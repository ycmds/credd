{
  "name": "credd",
  "version": "1.0.2",
  "type": "module",
  "description": "CLI tool for managing GitHub & GitLab & Local secrets and credentials",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "build:tsc": "           rm -rf lib && tsc",
    "build:tsup": "          tsup",
    "build:tsdown": "        tsdown",
    "build:bunup": "         bunup",
    "build:tsc:release": "   rm -rf lib && tsc --outDir lib",
    "build:tsdown:release": "tsdown --out-dir lib",
    "build": "               npm run build:tsdown:release",
    "build:all": "           npm run build:bunup && npm run build:tsup && npm run build:tsdown && npm run build:tsc",
    "dev": "                 tsdown --watch",
    "test:node": "           node --import=tsx --test tests/*.test.ts",
    "test:node:watch": "     node --import=tsx --test --watch tests/*.test.ts",
    "test:lint": "           biome lint",
    "test:size": "           echo skip size-limit",
    "test": "                npm run test:lint && npm run test:node && npm run test:size",
    "test:watch": "          npm run test:node:watch",
    "fix:lint": "            biome check --write",
    "fix": "                 npm run fix:lint",
    "version:patch": "       npm version patch && git push && git push --tags",
    "version:release": "     semantic-release --no-ci --dry-run ",
    "release": "             npm run build && npm run test && npm run version:release && npm publish"
  },
  "dependencies": {
    "@lsk4/err": "^4.26.0",
    "@lsk4/log": "^4.28.0",
    "@lsk4/stringify": "^4.28.0",
    "@ycmd/run": "^0.27.0",
    "@ycmd/utils": "^0.27.0",
    "axios": "^1.13.2",
    "fishbird": "^1.1.8",
    "libsodium-wrappers": "^0.7.15",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/node": "^25.0.3",
    "@types/yargs": "^17.0.32",
    "bunup": "^0.16.11",
    "semantic-release": "^25.0.2",
    "size-limit": "^11.2.0",
    "tsdown": "^0.18.3",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "5.9.3"
  },
  "//": "///////////========================/////////========================/////////========================/////////",
  "files": [
    "lib",
    "cjs",
    "LICENSE",
    "README.md"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "credd": "./lib/bin/credd.js"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "0.4kb"
    }
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "pnpm-lock.yaml",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ðŸŽ‰ v${nextRelease.version}\n\n${nextRelease.notes}\n\n[skip ci]"
        }
      ]
    ]
  },
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycmds/credd.git"
  },
  "homepage": "https://github.com/ycmds/credd",
  "bugs": "https://github.com/ycmds/credd/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "keywords": [
    "utility",
    "library",
    "typescript",
    "esm",
    "es6",
    "functional",
    "programming",
    "utils",
    "helpers"
  ],
  "////": "/////////========================/////////========================/////////========================/////////"
}
